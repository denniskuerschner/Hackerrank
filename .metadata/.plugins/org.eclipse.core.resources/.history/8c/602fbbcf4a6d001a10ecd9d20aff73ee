package com.test;

public class Test {

	public static void main(String[] args) {
        System.out.println(celsiusInFahrenheitUmwandeln(41));
    }

    public static double celsiusInFahrenheitUmwandeln(double celsius) {
        //schreib hier deinen Code
        double fahrenheit = celsius +32 / 5/9;
        return fahrenheit;
    }

	}


