package com.zuweisungHalter;

public class zuweisungHalter {
	public static void main(String[] args) {
        //schreib hier deinen Code
        Katze muschi = new Katze();
        Hund bello = new Hund();
        Fisch goldi = new Fisch();
        Frau halter = new Frau();
        muschi.halter = new Frau();
        bello.halter = new Frau();
        goldi.halter = new Frau();
       
    }

    public static class Katze {
        public Frau halter;
    }

    public static class Hund {
        public Frau halter;
    }

    public static class Fisch {
        public Frau halter;
    }

    public static class Frau {
    }

}
