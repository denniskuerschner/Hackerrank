package com.test;

public class Test {

	public static void main(String[] args) {
        System.out.println(celsiusInFahrenheitUmwandeln(41));
    }

    public static double celsiusInFahrenheitUmwandeln(int celsius) {
        //schreib hier deinen Code
        return celsius * 1.8 + 32;
    }

	}


